{
  "name": "okkino",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:db": "pnpm run db:dockers:dev",
    "start:web": "nx serve web",
    "start:web-admin": "nx serve web-admin --port 3000",
    "----DB SECTION----": "-------------------------",
    "db:dockers:dev": "docker-compose -f tools/okkino-env/dev.docker-compose.yaml up -d --no-recreate --remove-orphans",
    "db:migrate:dev": "pnpm dlx env-cmd -f .local.env pnpm dlx prisma migrate dev && prisma generate",
    "db:seed:dev": "pnpm dlx env-cmd -f .local.env ts-node --project tools/tsconfig.tools.json tools/database/seed.dev.ts",
    "db:studio": "pnpm dlx env-cmd -f .local.env pnpm dlx prisma studio",
    "----VALIDATE ENV SECTION----": "--------------------------------",
    "validate:web-env": "pnpm dlx nx run web-utils-env:validate",
    "validate:web-admin-env": "pnpm dlx nx run web-utils-env-admin:validate",
    "----TOOLS SECTION----": "-------------------------",
    "verify": "nx format:write && nx format:check && nx affected --target=lint --fix --parallel=3 && nx affected --target=build --parallel=3 --exclude=web",
    "stripe-listen": "stripe listen --forward-to localhost:4200/api/fulfilled",
    "----DEPLOY SECTION----": "-----------------------",
    "cd:web": "pnpm run validate:web-env && prisma generate && pnpm dlx nx build web --prod",
    "cd:web-admin": "pnpm run validate:web-admin-env && prisma migrate && prisma generate && pnpm run cd:db-seed && pnpm dlx nx build web-admin --prod",
    "cd:db-push": "pnpm dlx prisma db push --accept-data-loss",
    "cd:db-seed": "pnpm dlx ts-node --project tools/tsconfig.tools.json tools/database/seed.prod.ts"
  },
  "private": true,
  "dependencies": {
    "@clerk/nextjs": "^4.21.14",
